   "source": [
    "num=42\n",
    "b=format(num,'08b')\n",
    "f=''\n",
    "for i in b:\n",
    "    for j in ['0','1']:\n",
    "        if i==j:\n",
    "            if j=='0':\n",
    "                f+='1'\n",
    "            else:\n",
    "                f+='0'\n",
    "c=int(f,2)\n",
    "c+=1\n",
    "print(\"2's complement of -42 ->\",format(c,'08b'))\n",
    " \n",
    "\n"
   ]
  }
